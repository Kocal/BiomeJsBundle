    "type": "symfony-bundle",
    "keywords": ["biome", "biome.js", "symfony", "bundle"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kocal\\BiomeJsBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Hugo Alliaume",
            "email": "hugo@alliau.me"
        }
    ],
    "require": {
